{
    "name": "notifymehq/hipchat",
    "description": "Provides a hipchat gateway for notifyme",
    "license": "MIT",
    "keywords": ["notifymehq", "notifyme", "hipchat"],
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "~5.3",
        "notifymehq/notifyme": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "NotifyMeHQ\\Hipchat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotifyMeHQ\\Hipchat\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
